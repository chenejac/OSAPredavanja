<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="WebShop" transaction-type="RESOURCE_LOCAL">
    <provider>org.hibernate.ejb.HibernatePersistence</provider>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.Admin</class>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.Category</class>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.OrderItem</class>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.Product</class>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.ProductImage</class>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.PurchaseOrder</class>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.Supplier</class>
    <class>rs.ac.uns.ftn.informatika.osa.pr17.User</class>
    <properties>
      <property name="hibernate.connection.driver_class" value="com.mysql.jdbc.Driver"/>
      <property name="hibernate.connection.url" value="jdbc:mysql://localhost/osa1"/>
      <property name="hibernate.connection.username" value="osa1"/>
      <property name="hibernate.connection.password" value="osa1"/>
      <property name="hibernate.dialect" value="org.hibernate.dialect.MySQLInnoDBDialect"/>
      <property name="log4j.logger.net.sf.hibernate" value="ERROR"/>
    </properties>
  </persistence-unit>
</persistence>